      ],
      "class-variance-authority@0.7.1": [
        "node_modules/class-variance-authority"
      ],
      "figma:asset/*": [
        "src/assets/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "src/main.tsx",
    "vite.config.ts"
  ]
}
